class Solution {
    int missingNumber(int array[], int n) {
        // Your Code Here
        // Arrays.sort(array);
        // // int k=1;
        // int i=0;
        // for(i=0;i<array.length;i++)
        // {
        //     if(array[i]!=i+1)
        //     break;
        // }
        // return i+1;
        int sum1=n*(n+1)/2;
        int sum2=0;
        for(int i=0;i<array.length;i++)
        {
            sum2=sum2+array[i];
        }
        return sum1-sum2;
    }
}
